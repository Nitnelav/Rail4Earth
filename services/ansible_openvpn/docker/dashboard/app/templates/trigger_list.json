{
  "index": "sensor_yamnet_*",
  "size" : 100,
  "_source": false,
    "sort": [
        {
            "date": {
                "order": "desc"
            }
        }
    ],
    "query": {
    "bool": {
      "must": [{
          "range": {
            "date": {
              "gte": {{ start_time }},
              "lte": {{ end_time }},
              "format": "epoch_millis"
            }
          }
        }
      ]
    }
  },
  "docvalue_fields": [
    {
      "field": "date",
      "format": "epoch_millis"
    },
    {
      "field": "hwa.keyword"
    },
    {
      "field": "scores_perc.*"
    }
  ]
}