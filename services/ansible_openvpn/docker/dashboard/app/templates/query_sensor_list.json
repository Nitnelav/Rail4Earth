{
  "index": "sensor_location_*",
   "query": {
    "bool": {
      "must": [
        {
          "exists": {
            "field": "TPV.lat"
          }
        }
      ]
    }
  },
  "_source": false,
  "collapse": {
    "field": "hwa.keyword",
    "inner_hits": {
      "name": "most_recent",
      "fields": [
        "TPV.lat",
        "TPV.lon",
        "hwa",
        "date"
      ],
      "size": 1,
      "sort": [
        {
          "date": "desc"
        }
      ],
      "_source": false
    },
    "max_concurrent_group_searches": 4
  },
  "sort": [
    {
      "date": {
        "order": "desc"
      }
    }
  ],
  "from": 0
}
